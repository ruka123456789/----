 void setup()
{
  size(250,250);
  background(255,255,0);
  line(0,0,1000,1000); 
  line(width,0,0,height); 
  ellipse(120,120,120,120);
  ellipse(120,120,100,100);
  ellipse(120,120,80,80);
  ellipse(120,120,60,60);
  ellipse(120,120,40,40);
  ellipse(120,120,20,20);
  size(250, 250);
  frameRate(20);
}
void draw() {

}
  void mousePressed() {
  ellipse(mouseX, mouseY, 30, 30);
}
